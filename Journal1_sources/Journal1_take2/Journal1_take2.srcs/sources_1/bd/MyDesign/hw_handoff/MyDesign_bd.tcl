  set key_pressed_0 [ create_bd_port -dir I -from 4 -to 0 key_pressed_0 ]
  set pout [ create_bd_port -dir O -from 7 -to 0 pout ]
  set rst [ create_bd_port -dir I -type rst rst ]
  set sysclk [ create_bd_port -dir I sysclk ]

  # Create instance: bundle_splitter_dec_0, and set properties
